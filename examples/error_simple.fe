package example.error_simple;

domain SimpleError {
  Failed { code: i32 }
}

function might_fail(x: i32) -> i32 error SimpleError {
  if x < 0 {
    return err Failed { code: x };
  }
  return ok x;
}

function main() -> i32 effects [io] {
  println("error simple test:");
  println("done");
  return 0;
}
