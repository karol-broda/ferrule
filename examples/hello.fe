package example.hello;

domain IoError {
  NotFound { path: String }
  Denied { path: String }
  Interrupted { op: String }
}

use error IoError;

function add(x: i32, y: i32) -> i32 {
  return x + y;
}

function greet(name: String) -> String {
  const greeting = "hello";
  return greeting;
}

function main() -> i32 {
  const x: i32 = 10;
  const y: i32 = 20;
  const result = add(x, y);
  
  if result === 30 {
    return 0;
  } else {
    return 1;
  }
}

